<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.name %>
    </li>
  <% end %>
</ul>

<% if current_user %>
  Logged in as: <%= current_user.name %> | <%= link_to 'Log out', logout_path, method: :delete %> <br>
  <% else %>
  <%= link_to 'Log in', login_path %> <br>

  <%= link_to 'Sign up', new_user_path %> <br>
<% end %>
